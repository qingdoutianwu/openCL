name: Streaming_Unlock_Check
desc: Streaming unlock checks (ChatGPT/Netflix/Disney/YouTube Premium/Spotify/Claude/HBO Max/Prime Video + Summary)
icon: https://fastly.jsdelivr.net/gh/StashNetworks/misc@main/collapsed-tiles/img/netflix.png

tiles:
  - name: ChatGPT-Web-Check
    title: ChatGPT Web
    icon: https://fastly.jsdelivr.net/gh/StashNetworks/misc@main/collapsed-tiles/img/chatgpt.png
    collapsed: true
    url: https://chatgpt.com
    interval: 900

  - name: Netflix-Check
    title: Netflix
    icon: https://fastly.jsdelivr.net/gh/StashNetworks/misc@main/collapsed-tiles/img/netflix.png
    collapsed: true
    url: https://www.netflix.com
    interval: 900

  - name: Unlock-Summary
    title: Unlock Summary
    icon: https://logo.clearbit.com/chatgpt.com
    collapsed: true
    url: https://chatgpt.com
    interval: 900

  - name: Disney-Check
    title: Disney+
    icon: https://logo.clearbit.com/disneyplus.com
    collapsed: true
    url: https://www.disneyplus.com
    interval: 900

  - name: YouTube-Premium-Check
    title: YouTube Premium
    icon: https://logo.clearbit.com/youtube.com
    collapsed: true
    url: https://www.youtube.com/premium
    interval: 900

  - name: Spotify-Check
    title: Spotify
    icon: https://logo.clearbit.com/spotify.com
    collapsed: true
    url: https://open.spotify.com
    interval: 900

  - name: Claude-Check
    title: Claude
    icon: https://logo.clearbit.com/claude.ai
    collapsed: true
    url: https://claude.ai
    interval: 900

  - name: HBO-Max-Check
    title: HBO Max (Max)
    icon: https://logo.clearbit.com/max.com
    collapsed: true
    url: https://www.max.com
    interval: 900

  - name: PrimeVideo-Check
    title: Prime Video
    icon: https://logo.clearbit.com/primevideo.com
    collapsed: true
    url: https://www.primevideo.com
    interval: 900

script-providers:
  # 官方成熟脚本（StashNetworks 示例）
  ChatGPT-Web-Check:
    url: https://fastly.jsdelivr.net/gh/StashNetworks/misc@main/collapsed-tiles/script/chatgpt-web.js
    interval: 86400
  Netflix-Check:
    url: https://fastly.jsdelivr.net/gh/StashNetworks/misc@main/collapsed-tiles/script/netflix.js
    interval: 86400

  # 你的脚本（已托管到 GitHub，并通过 jsDelivr 引用）
  Unlock-Summary:
    url: https://fastly.jsdelivr.net/gh/qingdoutianwu/openCL@main/stash_tile_scripts_v5/unlock-summary.js
    interval: 86400
  Disney-Check:
    url: https://fastly.jsdelivr.net/gh/qingdoutianwu/openCL@main/stash_tile_scripts_v5/disney.js
    interval: 86400
  YouTube-Premium-Check:
    url: https://fastly.jsdelivr.net/gh/qingdoutianwu/openCL@main/stash_tile_scripts_v5/youtube-premium.js
    interval: 86400
  Spotify-Check:
    url: https://fastly.jsdelivr.net/gh/qingdoutianwu/openCL@main/stash_tile_scripts_v5/spotify.js
    interval: 86400
  Claude-Check:
    url: https://fastly.jsdelivr.net/gh/qingdoutianwu/openCL@main/stash_tile_scripts_v5/claude.js
    interval: 86400
  HBO-Max-Check:
    url: https://fastly.jsdelivr.net/gh/qingdoutianwu/openCL@main/stash_tile_scripts_v5/hbo-max.js
    interval: 86400
  PrimeVideo-Check:
    url: https://fastly.jsdelivr.net/gh/qingdoutianwu/openCL@main/stash_tile_scripts_v5/primevideo.js
    interval: 86400
